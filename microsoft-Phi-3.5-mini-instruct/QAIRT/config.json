{
    "input_model": { 
        "type": "QairtPreparedModel", 
        "model_path": "/local/mnt2/workspace/kromero/models/HF-Microsoft-Phi-3.5-mini-instruct"
    },
    "data_configs": [
        {
            "name": "wikitext2_eval",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "data_name": "wikitext",
                "subset": "wikitext-2-raw-v1", 
                "split": "train"
            },
            "pre_process_data_config": {
                "model_name": "microsoft/Phi-3.5-mini-instruct",
                "strategy": "join",
                "add_special_tokens": false,
                "max_seq_len": 4096,
                "max_samples": 128
            }
        }
    ],
    "passes": {
        "qgab": {
            "type": "QairtGenAIBuilder",
            "backend": "HTP",
            "soc_details": "chipset:SC8380XP",
            "cache_dir": "./cache/qairt",
            "log_level": "debug"
        }
    },
    "evaluators": {
        "evaluator": {
            "type": "QairtContainerEvaluator",
            "metrics": [
                { 
                    "name": "latency", 
                    "type": "latency", 
                    "sub_types": [ { "name": "avg" } ], 
                    "data_config": "wikitext2_eval" 
                }
            ]
        }
    },
    "evaluator": "evaluator",
    "evaluate_input_model": false,
    "log_severity_level": 1,
    "output_dir": "models/phi3_5-qnn",
    "cache_dir": "cache",
    "no_artifacts": true
}
